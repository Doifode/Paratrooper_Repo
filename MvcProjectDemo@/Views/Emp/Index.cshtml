@model List<MvcProjectDemo_.Models.Employee>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h2>Hello Mr.Kiran Good Morning </h2>
        <table class="table table-bordered table-striped" >
            <tbody>
                <tr>
                    <th>Employee ID</th>
                    <th>Employee Name</th>
                    <th>Employee Salary</th>
                </tr>
                @foreach (var employee in Model)
                {
                    <tr>
                        <td>@employee.Id</td>
                        <td>@employee.Name</td>
                        <td>@employee.Salary</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
